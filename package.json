{
  "name": "temperjs",
  "version": "0.0.1",
  "description": "State management library for React",
  "homepage": "https://temperjs.org",
  "repository": "github:ktripaldi/temperjs",
  "bugs": "https://github.com/ktripaldi/temperjs/issues",
  "keywords": [
    "temper",
    "temperjs",
    "state",
    "trait",
    "react",
    "reactjs",
    "recoil",
    "redux",
    "flux"
  ],
  "main": "lib/temper.js",
  "unpkg": "dist/temper.js",
  "module": "es/temper.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "types"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage types",
    "check-types": "tsc --noEmit",
    "build": "rollup -c",
    "test": "jest --config jestconfig.json",
    "test:watch": "yarn run test --watchAll",
    "test:cov": "yarn run test --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"**/*.md\"",
    "lint": "eslint --ext ts src"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-test-renderer": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.8.3"
  },
  "author": "Kenderson Tripaldi",
  "maintainers": [
    {
      "name": "Kenderson Tripaldi",
      "email": "kenderson.tripaldi@icloud.com"
    }
  ],
  "license": "MIT"
}
